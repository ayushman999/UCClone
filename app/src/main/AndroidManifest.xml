<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ayushman999.urbanclapclone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UrbanClapClone">
        <activity android:name=".Activity.SellerProfile"></activity>
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".Activity.HomeScreenSeller" />
        <activity android:name=".Activity.HomeScreenUser" />
        <activity android:name=".Activity.ForgotPassword" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.SplashScreen" />
        <activity android:name=".Activity.LoginChoice">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>